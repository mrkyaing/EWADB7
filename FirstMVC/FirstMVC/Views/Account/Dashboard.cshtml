<head>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous" />
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
<script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
</head>
<div class="container">
<h1>This is dashboard</h1>
@TempData["Msg"]
<p id="whatTime"></p>
<button id="btnShowTime" class="btn btn-success">
    Show me Time
</button>
</div>
<div>
    Search:<input type="text" id="txtSearch" class="form-control"/>
    <p id="myfriend"></p>
    <button id="btnSearchFriend" class="btn btn-primary">Search Friend</button>
</div>
<script>
    $("#btnShowTime").click(function(){
        var url="/account/tellmenow";
        //AJAX is here 
        $.get(url,null,function(res){
            $("#whatTime").html(res);
        });
    });
    $("#btnSearchFriend").click(function (){
        var searchfriend=$("#txtSearch").val();
        var url="/account/FindFriends";
        var data={
            friend:searchfriend
        };
        $.get(url,data,function(res){
            console.log(res);
            $("#myfriend").html(res).show(2000);
        });
    });
</script>