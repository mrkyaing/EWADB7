<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
          crossorigin="anonymous" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
            crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
</head>
<div>
    Order Id:<input type="number" id="txtOrderid" class="form-control"/>
    Unit Price:<input type="number" id="txtUnitPrice" class="form-control" />
    Quantity:<input type="number" id="txtQuantity" class="form-control" />
    <input type="button" value="make an Order" id="btnMakeOrder" class="btn btn-success" />
</div>
<script>
    $("#btnMakeOrder").on("click",function (evt){
        const url="/order/MakeOrder";
        var anOrder={};//Create a object 
        anOrder.Id=$("#txtOrderid").val();
        anOrder.UnitPrice=$("#txtUnitPrice").val();
        anOrder.Quantity=$("#txtQuantity").val();
        $.ajax({
            type:'POST',
            url:url,
            data:anOrder,
            success:function(res){
            alert(`Total Price${res}`);
            },
            failure:function(res){
            console.log(res);
            }
        });
    });
</script>